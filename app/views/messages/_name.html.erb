<div class="header">
  <% if worker_signed_in? %>
    <div class="row">
      <div class="col-3">
        <%= image_tag "no_image.png", size:"50x50" %>
      </div>
      <div class="col-9">
        <h4 class="pa-20"><%= room.employer.store_name %></h4>
      </div> 
    </div>
  <% elsif employer_signed_in? %>
    <div class="row">
      <div class="col-3">
        <%= image_tag "no_image.png", size:"50x50" %>
      </div>
      <div class="col-9">
        <h4 class="pa-20"><%= room.worker.fullname %></h4>
      </div> 
    </div>
  <% end %>
</div>